
<div id="dialog"></div>

<div id="sidechannel-container">
  <center>Side Channel</center>
  <div id="sidechannel"></div>
</div>

<div id="notes-container">
  <center>Implementor Notes</center>
  <p>verify flow: <a href="/flow">link</a> <a href="/flow?verify=pin">pin</a>
  <p>"oops" page: <a href="/flow">off</a> <a href="/flow?verifyLanding=oops">on</a>
  <div class="close"><a href="javascript: toggleAdmin();">x</a></div>
  <div id="notes"></div>
  <p>Current Accounts:</p>
  <div id="all-accounts"></div>
</div>

<!-- templates -->
<div id="templates" class="hidden">

  <div class="t1-create-signin">

    <x-tabbox>
      <x-tabs>
        <x-tab selected>Create Account</x-tab>
        <x-tab>Sign In</x-tab>
      </x-tabs>
      <x-tabpanels>
        <x-tabpanel selected="true">
          <form class="create">
            <label>Email: </label>
            <input type="text" name="email" class="email focus" />
            <label>Confirm email </label>
            <input type="text" name="email_confirm" />
            <br/>
            <label>Password: </label>
            <input type="password" name="password" />
            <p>By proceeding you agree with <a href="#">Terms of Service</a>
              and <a href="#">Privacy Policy</a>
            </p>

            <input type="submit" class="go" value="Create Account" />
          </form>
        </x-tabpanel>

        <x-tabpanel>
          <form class="login">
            <label>Email: </label>
            <input type="text" name="email" class="email focus" />

            <label>Password: </label>
            <input type="password" name="password" />

            <p>
              <a href="#" class="forgot" data-page="reset-password">Forgot password?</a>
            </p>

            <br/>
            <input type="submit" class="go" value="Sign In" />
          </form>
        </x-tabpanel>
      </x-tabpanels>
    </x-tabbox>

  </div>

  <div class="notes-t1-get-email">
  </div>

  <div class="t2-signed-in-page">
      <center><h2>Welcome!<br/> You are successfully signed in as:</h2>
        <p class="user">test</p>
        <p id="progress-message">Syncing your data...</p>
        <progress id="progress-meter"></progress>
        <p>You may close this tab and keep browsing or go to <a href="#" class="prefs">Preferences</a> to manage your account.</p>
      </center>
  </div>

  <div class="notes-t2-get-password">
  </div>

  <div class="verify">
    <h2>Please verify your email at:</h2>
    <p>
      <strong class='verify-email'></strong>.
    </p>
  </div>

  <div class="verified">
    <h3>Email verified!</h3>
    <h3>Now go to Firefox and sign in!</h3>
  </div>

  <div class="oops">
    <h2>Oops, you need to open that link in Firefox!</h2>
    <img src="/images/facepalm.jpg" alt="failfox" />
  </div>

  <div class="reset-password">
    <p>
      Forgot your password? No worries!<br/>
      Reset your password!
    </p>
    <form class="reset">
      <label>Your Email:
        <input type="text" name="email" class="email focus" />
      </label>
      <p>
      <input type="submit" class="go right" value="Reset Password" />
    </form>
  </div>

  <div class="confirm-reset-code">
    <p>
      Reset password email sent to: <span class="email"></span>
    </p>
    <form class="reset_code">
      <label>Enter the code in the email:</label>
        <input type="text" name="code" class="code focus" />
      <p>
      <input type="submit" class="go right" value="Next" />
    </form>
  </div>

  <div class="new-password">
    <form class="new_password">
      <p>
        Create new password:
      </p>
      <input type="password" name="password" placeholder="new password" class="focus" /> <br/>
      <input type="password" name="confirm_password" placeholder="confirm password" />
      <p>
      <input type="submit" class="go right" value="Reset" />
    </form>
  </div>

  <div class="reset-success">
    <p>You reset your password successfully!
    <p>Remember to sign in using your new password on your other devices so they can be all synced!

    <ul class="devices">
    </ul>
  </div>

  <div class="confirm-email-code">
    <p>
      We sent an email to <span class="email"></span><br/>
      Enter the code from the email below:
    </p>
    <form class="email_code">
      <label>Type the code in your email:</label>
        <input type="text" name="code" class="code focus" />
      <p>
      <input type="submit" class="go right" value="Next" />
    </form>
  </div>

  <div class="preferences">
      <center><h2>Preferences</h2></center>
      <a href="#" class="logout">Sign out</a>
  </div>

  <div class="notes-preferences">
  </div>

</div>


 <script type="text/javascript" src="/javascripts/one.js"></script>
